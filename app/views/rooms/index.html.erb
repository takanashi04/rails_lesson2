<h1>宿泊一覧</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>画像</th>
      <th>施設名</th>
      <th>詳細</th>
      <th>値段</th>
      <th>住所</th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room|%>
      <tr>
        <td><%= room.id %></td>
        <td><%= image_tag('default-image.png') %></td>
        <td><%= room.name %></td>
        <td><%= room.content%></td>
        <td><%= room.price%></td>
        <td><%= room.address%></td>
      </tr>
    <% end %>
  </tbody>
</table>
